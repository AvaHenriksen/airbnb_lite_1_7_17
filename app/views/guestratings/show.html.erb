<div class="page-header">
  <h3>Guestrating #<%= @guestrating.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Host</dt>
      <dd><%= @guestrating.host_id %></dd>

      <dt>Guest</dt>
      <dd><%= @guestrating.guest_id %></dd>

      <dt>Booking</dt>
      <dd><%= @guestrating.booking_id %></dd>

      <dt>Body</dt>
      <dd><%= @guestrating.body %></dd>

      <dt>Rating</dt>
      <dd><%= @guestrating.rating %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/guestratings" class="btn btn-primary">
        Back
      </a>
      <a href="/guestratings/<%= @guestrating.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_guestrating/<%= @guestrating.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
